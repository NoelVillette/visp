{
  "preprocessor":
  {
    "defines": {
      "VISP_EXPORT": "",
      "vp_deprecated": "",
      "DOXYGEN_SHOULD_SKIP_THIS": "1",
      "NLOHMANN_JSON_SERIALIZE_ENUM(a,...)": "void ignored() {}",
      "__cplusplus" : "1"
    },
    "passThroughIncludesRegex": "^((?!vpConfig\\.h|opencv_modules\\.hpp|visp_modules\\.h).)*$",
    "neverDefine": [
      "VISP_BUILD_DEPRECATED_FUNCTIONS"
    ],
    "otherArguments": [
      "--passthru-unfound-includes", "--passthru-comments","--line-directive", ""
    ]
  }
}